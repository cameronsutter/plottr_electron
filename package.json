{
  "name": "plottr",
  "version": "2021.3.9-beta",
  "description": "A visual story-planning tool",
  "repository": "https://github.com/cameronsutter/plottr_electron.git",
  "homepage": "plottr.com",
  "private": true,
  "readme": "Copyright 2016-2020 Fictional Devices LLC",
  "author": {
    "name": "C. Louis S.",
    "email": "cameronsutter0@gmail.com",
    "url": "https://medium.com/@camsutter"
  },
  "main": "bin/main.js",
  "scripts": {
    "start": "NODE_ENV=dev electron .",
    "start-win": "set NODE_ENV=dev&& electron .",
    "compile": "NODE_ENV=dev node_modules/.bin/webpack",
    "watch": "echo 'do you have fsevents?' && NODE_ENV=dev node_modules/.bin/webpack",
    "watch-win": "set NODE_ENV=dev&& node_modules\\.bin\\webpack",
    "createDevLicense": "node bin/main_modules/dev_license.js",
    "changelog": "changelog cameronsutter/plottr_electron all -m > changelog.md",
    "maps": "NODE_ENV=production MAPS=true node_modules/.bin/webpack",
    "pack": "NODE_ENV=production node_modules/.bin/webpack",
    "pack:windows": "node_modules\\.bin\\webpack",
    "dist": "node build/initEnv.js && npm run pack && electron-builder -ml",
    "dist:linux": "node build/initEnv.js && npm run pack && electron-builder -l",
    "dist:windows": "node build/initEnv.js && npm run pack:windows && electron-builder -w",
    "dist:all": "npm run pack && electron-builder -mw",
    "extract-strings": "format-message extract -o locales/en.json \"bin/**/*.js\" \"src/**/*.js\" \"shared/**/*.js\" \"lib/**/*.js\" && npm run generate-flipped",
    "generate-flipped": "node ./locales/build-flipped-locale.js",
    "test": "jest '(src.*)|(lib.*)'",
    "test:watch": "jest '(src.*)|(lib.*)' --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand src/app/components/rce/__tests__/RichTextEditor.test.js",
    "test:integration": "jest test/integration",
    "start-redux-dev-tools": "redux-devtools --hostname=localhost --port=8000",
    "lint-file": "eslint",
    "lint": "eslint .",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "validate": "npm-run-all format:check lint test"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "dotenv-stringify": "^1.0.0",
    "electron-context-menu": "^2.0.1",
    "electron-differential-updater": "^4.3.2",
    "electron-json-storage": "^2.0.0",
    "electron-log": "^2.2.17",
    "electron-process-manager": "^1.0.0",
    "electron-prompt": "^1.5.1",
    "electron-store": "^5.1.0",
    "electron-util": "^0.14.0",
    "electron-window-state": "^5.0.3",
    "format-message": "^6.2.1",
    "rollbar": "^2.19.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.4.1",
    "ajv": "^6.12.2",
    "dom-parser": "^0.1.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "changelog": "^1.4.2",
    "classnames": "^2.2.6",
    "css-loader": "^3.0.0",
    "devtron": "^1.2.1",
    "docx": "^5.0.2",
    "electron": "^10.2.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-notarize": "^1.0.0",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^0.8.5",
    "flip": "^1.0.0",
    "format-message-cli": "^6.2.1",
    "image-extensions": "^1.1.0",
    "is-hotkey": "^0.1.6",
    "is-url": "^1.2.4",
    "jest": "^26.6.3",
    "jsrtf": "0.0.3",
    "lodash": "^4.17.19",
    "mixpanel-browser": "^2.38.0",
    "node-machine-id": "^1.1.12",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pagedown": "^1.1.0",
    "pltr": "file:./lib/pltr",
    "prettier": "^2.2.1",
    "pretty-date": "^0.2.0",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.8.6",
    "react-floater": "^0.7.2",
    "react-icons": "^3.11.0",
    "react-image-file-resizer": "^0.2.1",
    "react-json-inspector": "^7.1.1",
    "react-json-view": "^1.19.1",
    "react-modal": "^3.8.1",
    "react-test-renderer": "^17.0.1",
    "react-overlays": "^1.2.0",
    "react-proptypes": "^1.0.0",
    "react-redux": "^4.4.10",
    "react-sticky-table": "^4.3.1",
    "react-waypoint": "^9.0.2",
    "react.pure.component": "^1.2.0",
    "reactour": "^1.16.0",
    "redux": "^3.5.2",
    "redux-devtools-cli": "^1.0.0-4",
    "redux-undo": "^1.0.1",
    "remote-redux-devtools": "^0.5.16",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.9",
    "reselect": "^4.0.0",
    "sass-loader": "^7.1.0",
    "semver": "^7.1.3",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-hyperscript": "^0.59.0",
    "slate-react": "^0.59.0",
    "style-loader": "^0.13.0",
    "url-loader": "^2.3.0",
    "uuid": "^8.3.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.5",
    "xml-js": "^1.6.11"
  },
  "build": {
    "electronVersion": "10.1.5",
    "appId": "com.plottr.app",
    "artifactName": "${productName}-${os}-installer-latest.${ext}",
    "asar": false,
    "publish": [
      "github"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc,md,MD}",
      "!**/._*",
      "!.vscode${/*}",
      "!**/{.DS_Store,.git,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!src${/*}",
      "!webpack.config.js",
      "!dev-app-update.yml",
      "!bin/*.map",
      "!examples${/*}"
    ],
    "copyright": "Copyright Â© 2020 Fictional Devices",
    "productName": "Plottr",
    "fileAssociations": [
      {
        "ext": "pltr",
        "name": "Plottr",
        "description": "An elegant writing tool to organize story plotlines",
        "role": "Editor"
      }
    ],
    "protocols": [
      {
        "name": "Plottr URL",
        "schemes": [
          "plottr"
        ],
        "role": "Editor"
      }
    ],
    "afterSign": "build/notarize.js",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "default",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "darkModeSupport": true
    },
    "dmg": {
      "backgroundColor": "#ebf2f9",
      "title": "${productName} Installer",
      "sign": false
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }
    },
    "nsis": {
      "artifactName": "${productName}-${os}-installer-latest.${ext}",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office",
      "icon": "build/icon1024x1024.png",
      "vendor": "Fictional Devices",
      "desktop": {
        "Name": "Plottr",
        "Type": "Application"
      }
    }
  }
}
