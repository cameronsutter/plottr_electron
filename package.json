    "firebase": "^9.9.3",
    "firebase-admin": "^9.12.0",
    "firebase-tools": "^9.23.3",
    "firebaseui": "^6.0.1",
    "format-message": "^6.2.4",
    "image-conversion": "^2.1.1",
    "immer": "^9.0.18",
    "invariant": "^2.2.4",
    "is-buffer": "^2.0.5",
    "is-lite": "^0.8.0",
    "is-plain-object": "^5.0.0",
    "jsdom": "^16.7.0",
    "json-schema-traverse": "^1.0.0",
    "jsonfile": "^6.1.0",
    "keycode": "^2.2.1",
    "luxon": "^2.5.2",
    "node-fetch-cjs": "^3.2.10",
    "path-exists": "^4.0.0",
    "prop-types": "^15.8.1",
    "prop-types-extra": "^1.1.1",
    "react-is": "^16.13.1",
    "react-overlays": "^5.2.1",
    "react-tiny-popover": "^7.2.0",
    "react-transition-group": "^4.4.5",
    "redux-thunk": "^2.3.0",
    "rtf.js": "^3.0.7",
    "slate": "^0.80.0",
    "slate-history": "^0.66.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.80.0",
    "tiny-invariant": "^1.0.6",
    "uncontrollable": "^7.2.1",
    "utf-8-validate": "^5.0.9",
    "warning": "^4.0.3",
    "ws": "^8.5.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "changelog": "^1.4.2",
    "circular-dependency-plugin": "^5.2.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^3.0.0",
    "dom-parser": "^0.1.6",
    "dotenv-webpack": "^7.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.0.4",
    "electron-notarize": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^0.8.5",
    "flip": "^1.0.0",
    "format-message-cli": "^6.2.1",
    "hash-test-vectors": "^1.3.2",
    "image-extensions": "^1.1.0",
    "is-hotkey": "^0.1.8",
    "is-url": "^1.2.4",
    "jest": "^26.6.3",
    "jsrtf": "^0.0.3",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.38.0",
    "net": "^1.0.2",
    "node-machine-id": "^1.1.12",
    "npm-run-all": "^4.1.5",
    "pagedown": "^1.1.0",
    "plottr_firebase": "file:./lib/plottr_firebase",
    "plottr_import_export": "file:./lib/plottr_import_export",
    "plottr_locales": "file:./lib/plottr_locales",
    "plottr_world": "file:./lib/plottr_world",
    "pltr": "file:../pltr",
    "prettier": "^2.2.1",
    "pretty-date": "^0.2.0",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^16.14.0",
