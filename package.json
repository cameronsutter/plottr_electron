{
  "name": "plottr",
  "version": "2023.1.10-alpha.3",
  "description": "A visual story-planning tool",
  "repository": "https://github.com/cameronsutter/plottr_electron.git",
  "homepage": "plottr.com",
  "private": true,
  "readme": "Copyright 2016-2020 Fictional Devices LLC",
  "author": {
    "name": "Cameron Sutter",
    "email": "cameronsutter0@gmail.com",
    "url": "https://twitter.com/camsutter"
  },
  "main": "bin/electron_main.js",
  "scripts": {
    "spit-out-version": "node build/spit-out-version.js",
    "start": "cross-env NODE_ENV=dev electron --inspect=5858 --enable-test-utilities .",
    "postinstall": "",
    "compile": "cross-env NODE_ENV=dev node_modules/.bin/webpack",
    "bundle-stats": "NODE_ENV=production node_modules/.bin/webpack --profile --json > stats.json",
    "watch": "npm-run-all --parallel watch:*",
    "watch:plottr": "echo 'do you have fsevents?' && cross-env NODE_ENV=dev node_modules/.bin/webpack",
    "changelog": "changelog cameronsutter/plottr_electron all -m > changelog.md",
    "maps": "cross-env NODE_ENV=production MAPS=true node_modules/.bin/webpack",
    "pack": "cross-env NODE_ENV=production node_modules/.bin/webpack",
    "pack:windows": "cross-env NODE_ENV=production node_modules\\.bin\\webpack",
    "dist": "npm run spit-out-version && node build/initEnv.js && npm run pack && electron-builder -ml --publish always",
    "dist:linux": "npm run spit-out-version && node build/initEnv.js && npm run pack && electron-builder -l",
    "dist:windows": "npm run spit-out-version && node build/initEnv.js && npm run pack:windows && electron-builder -w --publish always",
    "dist:all": "npm run pack && electron-builder -mw",
    "extract-strings": "lib/plottr_locales/lib/format-message-cli/format-message extract -o lib/plottr_locales/src/en-extracted.json \"bin/**/*.js\" \"src/**/*.js\" \"shared/**/*.js\" \"lib/plottr_components/src/**/*.js\" \"lib/pltr/v2/**/*.js\" && npm --prefix lib/plottr_locales run sync && npm run generate-flipped",
    "generate-flipped": "node ./lib/plottr_locales/src/build-flipped-locale.js",
    "test": "jest \"(src.*)|(lib.*)|(main.*)\" && jest --runInBand --config=jest.integration.config.js \"(src.*)|(lib.*)|(main.*)\"",
    "test:watch": "jest \"(src.*)|(lib.*)|(main.*)\" --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand lib/pltr/v2/reducers/__tests__/ui.test.js",
    "test:integration": "jest test/integration",
    "lint-file": "eslint",
    "lint": "eslint .",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "validate": "npm-run-all format:check lint test",
    "import-test": "npm run test:aCoronersTale && npm run test:fractureTest && npm run test:theTurn && npm run test:import-scrivener",
    "test:watch-import-scrivener": "jest \"lib/plottr_import_export/src/importer/scrivener/__tests__/importer.test.js\" --watch",
    "test:aCoronersTale": "electron --inspect=5858 . --import-from-scrivener ./examples/A-Coroners-Tale-Temp2.scriv --output-file ./examples/test_output/A-Coroners-Tale-Temp2.pltr",
    "test:fractureTest": "electron --inspect=5858 . --import-from-scrivener ./examples/Fractured-Test-Import-to-Plottr.scriv --output-file ./examples/test_output/Fractured-Test-Import-to-Plottr.pltr",
    "test:theTurn": "electron --inspect=5858 . --import-from-scrivener ./examples/The-Turn.scriv --output-file ./examples/test_output/The-Turn.pltr",
    "clean-all": "npm-run-all --parallel clean:*",
    "clean:plottr_electron": "rimraf node_modules/",
    "clean:pltr": "rimraf lib/pltr/node_modules",
    "clean:plottr_locales": "rimraf lib/plottr_locales/node_modules",
    "clean:plottr_world": "rimraf lib/plottr_world/node_modules",
    "clean:plottr_firebase": "rimraf lib/plottr_firebase/node_modules",
    "clean:plottr_import_export": "rimraf lib/plottr_import_export/node_modules",
    "regen-all": "npm-run-all --serial regen:plottr_import_export regen:plottr_locales regen:plottr_world regen:pltr regen:plottr_firebase regen:this",
    "regen:this": "rimraf package-lock.json && npm i",
    "regen:pltr": "rimraf lib/pltr/package-lock.json && npm --prefix lib/pltr i",
    "regen:plottr_locales": "rimraf lib/plottr_locales/package-lock.json && npm --prefix lib/plottr_locales i",
    "regen:plottr_world": "rimraf lib/plottr_world/yarn.lock && yarn --cwd lib/plottr_world install",
    "regen:plottr_firebase": "rimraf lib/plottr_firebase/yarn.lock && yarn --cwd lib/plottr_firebase install",
    "regen:plottr_import_export": "rimraf lib/plottr_import_export/package-lock.json && npm --prefix lib/plottr_import_export i",
    "ci-all": "npm-run-all --serial ci:plottr_import_export ci:plottr_locales ci:plottr_world ci:pltr ci:plottr_firebase ci:this",
    "ci:this": "npm ci",
    "ci:pltr": "npm --prefix lib/pltr ci",
    "ci:plottr_locales": "npm --prefix lib/plottr_locales ci",
    "ci:plottr_world": "yarn --cwd lib/plottr_world install",
    "ci:plottr_firebase": "yarn --cwd lib/plottr_firebase install",
    "ci:plottr_import_export": "yarn --cwd lib/plottr_import_export install"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "animated-scroll-to": "^2.3.0",
    "axios": "^0.22.0",
