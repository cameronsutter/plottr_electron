  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "build": {
    "electronVersion": "18.3.0",
    "appId": "com.plottr.app",
    "artifactName": "${productName}-${os}-installer-latest.${ext}",
    "npmRebuild": false,
    "publish": [
      "github"
    ],
    "files": [
      "**/*",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc,md,MD}",
      "!**/._*",
      "!.vscode${/*}",
      "!**/{.DS_Store,.git,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!src${/*}",
      "!*.config.js",
      "!dev-app-update.yml",
      "!bin/*.map",
      "!examples${/*}",
      "!package.json",
      "!test${/*}",
      "!lib/plottr_locales/build-flipped-locale.js",
      "!lib/${/*}",
      "!main${/*}",
      "!node_modules${/*}"
    ],
    "copyright": "Copyright © 2022 Fictional Devices",
    "productName": "Plottr",
    "fileAssociations": [
      {
        "ext": "pltr",
        "name": "Plottr",
        "description": "An elegant writing tool to organize story plotlines",
        "role": "Editor"
      }
    ],
    "protocols": [
      {
        "name": "Plottr URL",
        "schemes": [
          "plottr"
        ],
        "role": "Editor"
      }
    ],
    "afterSign": "build/notarize.js",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "default",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "darkModeSupport": true
    },
    "dmg": {
      "backgroundColor": "#ebf2f9",
      "title": "${productName} Installer",
      "sign": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "verifyUpdateCodeSignature": false,
      "publisherName": [
        "C Louis S, LLC",
        "Fictional Devices LLC"
      ]
    },
    "nsis": {
      "artifactName": "${productName}-${os}-installer-latest.${ext}",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office",
      "icon": "build/icon1024x1024.png",
      "vendor": "Fictional Devices",
      "desktop": {
        "Name": "Plottr",
        "Type": "Application"
      }
    }
  }
}
